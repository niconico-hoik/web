{
  "DEVELOPMENT": {
    "put": "./frame",
    "out": "./_local",
    "process": {},
    "weirs": [
      ["./onerror.js", { "js": ["EmbedJs", "utf8"] }],
      ["./post", { "md": ["MarkdownToHtml", "utf8"] }],
      ["./svg", { "svg": "OptimizeSvg" }],
      ["./image", { "jpg": "OptimizeJpg", "png": "OptimizePng" }]
    ],
    "ignore": {
      "targets": [
        "./favicons.png",
        "./ganalytics.js",
        "./robots.txt",
        "./sitemap.xml"
      ]
    }
  },

  "PRODUCTION:LIGHT": {
    "put": "./frame",
    "out": "./_site",
    "process": {},
    "weirs": [
      ["./onerror.js", { "js": ["EmbedJs", "utf8"] }],
      ["./post", { "md": ["MarkdownToHtml", "utf8"] }],
      ["./svg", { "svg": "OptimizeSvg" }]
    ],
    "ignore": {
      "targets": [
        "./favicons.png",
        "./ganalytics.js",
        "./robots.txt",
        "./sitemap.xml"
      ]
    }
  },

  "PRODUCTION:MASTER": {
    "put": "./frame",
    "out": "./_site",
    "process": {},
    "weirs": [
      ["./ganalytics.js", { "js": ["EmbedJs", "utf8"] }],
      ["./favicons.png", { "png": "EmbedFavicons" }],
      ["./post", { "md": ["MarkdownToHtml", "utf8"] }],
      ["./svg", { "svg": "OptimizeSvg" }]
    ],
    "ignore": {
      "targets": [
        "./onerror.js"
      ]
    }
  },

  "INKSCHIN": {
    "put": "./ikc",
    "out": "./frame",
    "weirs": [
      ["./favicons", { "svg": ["InkPngWhite", null, "stream"] }],
      ["./image", { "svg": ["InkPngWhite", null, "stream"] }],
      ["./pdf", { "svg": ["InkPdf", null, "stream"] }]
    ]
  }
}
