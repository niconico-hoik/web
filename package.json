    "chin-plugin-imagemin": "^0.0.4",
    "chin-plugin-inkscape": "^0.0.7",
    "chin-plugin-unified": "^0.0.4",
    "cross-env": "^5.2.0",
    "cross-var": "^1.1.0",
    "css-loader": "^1.0.0",
    "dotenv-webpack": "^1.5.7",
    "env-cmd": "^8.0.2",
    "execa": "^1.0.0",
    "flow-bin": "^0.79.1",
    "flow-typed": "^2.5.1",
    "http-server": "^0.11.1",
    "husky": "^0.14.3",
