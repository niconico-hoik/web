{
  "lonogara": "github.com/kthjm/lonogara.git",
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  },
  "scripts": {
    "format": "prettier --write **/*.js **/*.md **/*.json",
    "pipe": "chin -c .scripts/chin.config.json -p .scripts/chin.preset.js -v",
    "ikc": "yarn pipe INKSCHIN",
    "dev:pipe": "yarn pipe DEVELOPMENT",
    "dev:dll": "webpack --config .scripts/webpack.config.dll.babel.js",
    "dev:pre": "run-s dev:pipe dev:dll",
    "dev": "webpack-dev-server --config .scripts/webpack.config.dev.babel.js",
    "pro:pipe": "yarn pipe PRODUCTION",
    "pro:pack": "webpack -p --config .scripts/webpack.config.pro.babel.js",
    "pro": "run-s pro:pipe pro:pack",
    "pro:serve": "http-server .site -a localhost -p 7000 -o -c-1",
    "pro:nfy": "run-s lonogara:add pro",
    "lonogara:add": "env-cmd ./.env cross-var yarn add git+https://$GITHUB_ACCESS_TOKEN:x-oauth-basic@$npm_package_lonogara",
    "lonogara:rm": "yarn remove lonogara"
  },
  "dependencies": {
    "atra": "^1.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "lonogara-tool": "https://github.com/kthjm/lonogara-tool",
    "moment": "^2.19.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-parade": "https://github.com/kthjm/react-parade",
    "react-vertical-center": "https://github.com/kthjm/react-vertical-center",
    "unist-util-find": "^1.0.1",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "chin": "^2.1.2",
    "chin-plugins-inkscape": "https://github.com/kthjm/chin-plugins-inkscape",
    "cross-var": "^1.1.0",
    "css-loader": "^0.28.7",
    "dotenv-webpack": "^1.5.4",
    "env-cmd": "^7.0.0",
    "favicons": "^4.8.6",
    "flow-bin": "^0.61.0",
    "flow-typed": "^2.2.1",
    "fs-extra": "^4.0.3",
    "http-server": "^0.10.0",
    "imagemin": "^5.3.1",
    "imagemin-jpegtran": "^5.0.2",
    "imagemin-pngquant": "^5.0.1",
    "imagemin-svgo": "^6.0.0",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.9.1",
    "rehype": "^5.0.1",
    "rehype-format": "^2.2.0",
    "rehype-react": "^3.0.2",
    "remark": "^8.0.0",
    "remark-rehype": "^3.0.0",
    "unified": "^6.1.6",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.0"
  }
}
