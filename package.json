{
  "name": "niconico-hoik-web",
  "author": "kthjm <toxictoxer@gmail.com>",
  "repository": "niconico-hoik/web",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "nodule": "cross-env BABEL_ENV=cjs node -r babel-register",
    "hook": "yarn nodule hook.js",
    "prestart": "cross-env HOOK_ENV=prestart yarn hook",
    "prebuild": "cross-env HOOK_ENV=prebuild yarn hook",
    "postbuild": "http-server _site -a localhost -p 7000 -o -c-1",
    "chin:pre": "cross-env BABEL_ENV=cjs chin -c -r babel-register",
    "chin:dev": "cross-env BABEL_ENV=cjs chin watch -c -r babel-register",
    "chin:mir": "cross-env BABEL_ENV=cjs chin -c -r babel-register",
    "chin:pro": "cross-env BABEL_ENV=cjs chin -c -r babel-register",
    "wpack:dll": "webpack",
    "wpack:dev": "cross-env BABEL_ENV=cjs webpack-dev-server",
    "wpack:pro": "cross-env BABEL_ENV=esm webpack",
    "wpack:non": "cross-env BABEL_ENV=esm webpack",
    "wpack:ana": "cross-env BABEL_ENV=esm webpack --display-used-exports",
    "wpack:spl": "cross-env BABEL_ENV=esm webpack",
    "start": "run-p chin:dev wpack:dev",
    "build": "run-s chin:mir wpack:pro",
    "build:mirror": "run-s chin:mir wpack:pro",
    "build:master": "run-s chin:pro wpack:pro"
  },
  "dependencies": {
    "atra": "^1.2.3",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "lonogara-sdk": "https://github.com/lonogara/sdk#0.0.8",
    "moment": "^2.20.1",
    "niconico-hoik-price": "https://github.com/niconico-hoik/price#0.0.4",
    "orph": "^3.0.7",
    "react": "^16.2.0",
    "react-centpn": "^0.0.6",
    "react-dom": "^16.2.0",
    "react-html-parser": "^2.0.2",
    "react-lonogara": "https://github.com/lonogara/react-lonogara",
    "react-parade": "^0.0.1",
    "react-player": "^1.1.1",
    "redam": "^0.0.7",
    "screenfull": "^3.3.2",
    "tumblrinbrowser": "^0.0.3",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "chin": "^3.1.1",
    "chin-plugin-favicons": "^0.0.1",
    "chin-plugin-imagemin": "^0.0.2",
    "chin-plugin-inkscape": "^0.0.5",
    "chin-plugin-unified": "^0.0.2",
    "cross-env": "^5.1.3",
    "cross-var": "^1.1.0",
    "css-loader": "^0.28.9",
    "dotenv-webpack": "^1.5.4",
    "env-cmd": "^7.0.0",
    "execa": "^0.9.0",
    "flow-bin": "^0.63.1",
    "flow-typed": "^2.2.1",
    "http-server": "^0.11.1",
    "husky": "^0.14.3",
    "npm-run-all": "^4.1.2",
    "path-exists": "^3.0.0",
    "rehype-format": "^2.2.0",
    "webpack": "^4.8.3",
    "webpack-bundle-analyzer": "^2.12.0",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2"
  }
}
