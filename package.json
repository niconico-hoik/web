{
  "name": "lonogara-app-nicohoi",
  "author": "kthjm <toxictoxer@gmail.com>",
  "repository": "kthjm/lonogara-app-nicohoi",
  "license": "MIT",
  "lonogara": "github.com/kthjm/lonogara.git#0",
  "scripts": {
    "nodule": "cross-env BABEL_ENV=cjs node -r babel-register",
    "hook": "yarn nodule hook.js",
    "precommit": "cross-env HOOK_ENV=precommit yarn hook",
    "prestart": "cross-env HOOK_ENV=prestart yarn hook",
    "prebuild": "cross-env HOOK_ENV=prebuild yarn hook",
    "postbuild": "http-server _site -a localhost -p 7000 -o -c-1",
    "pipe": "cross-env BABEL_ENV=cjs chin -c -r babel-register",
    "pipe:pre": "cross-env CHIN_ENV=inkschin yarn pipe",
    "pipe:dev": "cross-env CHIN_ENV=development yarn pipe",
    "pipe:mir": "cross-env CHIN_ENV=mirror yarn pipe",
    "pipe:pro": "cross-env CHIN_ENV=production yarn pipe",
    "wpack:dll": "webpack",
    "wpack:dev": "cross-env BABEL_ENV=cjs webpack-dev-server",
    "wpack:pro": "cross-env BABEL_ENV=esm webpack -p",
    "wpack:net": "run-s lonogara:add wpack:pro",
    "start": "run-s pipe:dev wpack:dev",
    "build": "run-s pipe:mir wpack:pro",
    "build:mirror": "run-s pipe:mir wpack:net",
    "build:master": "run-s pipe:pro wpack:net",
    "lonogara:add": "env-cmd ./.env cross-var yarn add git+https://$LONOGARA_TOKEN@$npm_package_lonogara"
  },
  "dependencies": {
    "atra": "^1.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "lonogara-sdk": "https://github.com/kthjm/lonogara-sdk#0",
    "moment": "^2.20.1",
    "nicohoi-price-api": "https://github.com/kthjm/nicohoi-price-api#0",
    "orph": "^3.0.2",
    "react": "^16.2.0",
    "react-centpn": "^0.0.1",
    "react-dom": "^16.2.0",
    "react-parade": "^0.0.1",
    "react-player": "^1.1.1",
    "screenfull": "^3.3.2",
    "unist-util-find": "^1.0.1",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "chin": "^3.0.5",
    "chin-plugin-favicons": "^0.0.1",
    "chin-plugin-imagemin": "^0.0.2",
    "chin-plugin-inkscape": "^0.0.5",
    "chin-plugin-unified": "^0.0.2",
    "cross-env": "^5.1.3",
    "cross-var": "^1.1.0",
    "css-loader": "^0.28.9",
    "dotenv-webpack": "^1.5.4",
    "env-cmd": "^7.0.0",
    "execa": "^0.9.0",
    "flow-bin": "^0.63.1",
    "flow-typed": "^2.2.1",
    "http-server": "^0.11.1",
    "husky": "^0.14.3",
    "npm-run-all": "^4.1.2",
    "path-exists": "^3.0.0",
    "rehype": "^5.0.1",
    "rehype-format": "^2.2.0",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.0"
  }
}
