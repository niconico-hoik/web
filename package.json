{
  "lonogara": "github.com/kthjm/lonogara.git#0",
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  },
  "scripts": {
    "format": "prettier --write **/*.js **/*.md **/*.json",
    "pipe": "chin -c .scripts/chin.config.json -p .scripts/chin.preset.js -v",
    "ikc": "cross-env BABEL_ENV=cjs yarn pipe INKSCHIN",
    "dev:pipe": "cross-env BABEL_ENV=cjs yarn pipe DEVELOPMENT",
    "dev:dll": "webpack --config .scripts/webpack.config.dll.js",
    "dev:pre": "run-s dev:pipe dev:dll",
    "dev": "cross-env BABEL_ENV=cjs webpack-dev-server --config .scripts/webpack.config.dev.js",
    "dev:all": "run-s dev:pre dev",
    "pro:pipe": "yarn pro:pipe_light",
    "pro:pipe_light": "cross-env BABEL_ENV=cjs yarn pipe PRODUCTION:LIGHT",
    "pro:pipe_master": "cross-env BABEL_ENV=cjs yarn pipe PRODUCTION:MASTER",
    "pro:pack": "cross-env BABEL_ENV=esm webpack -p --config .scripts/webpack.config.pro.js",
    "pro:serve": "http-server .site -a localhost -p 7000 -o -c-1",
    "pro": "run-s pro:pipe_light pro:pack pro:serve",
    "lonogara:add": "env-cmd ./.env cross-var yarn add git+https://$GITHUB_ACCESS_TOKEN:x-oauth-basic@$npm_package_lonogara",
    "precommit": "cross-env BABEL_ENV=cjs node --require babel-register .scripts/precommit.js",
    "production:light": "run-s lonogara:add pro:pipe_light pro:pack",
    "production:master": "run-s lonogara:add pro:pipe_master pro:pack"
  },
  "dependencies": {
    "atra": "^1.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "lonogara-tool": "https://github.com/kthjm/lonogara-tool#0",
    "moment": "^2.20.1",
    "nicohoi-price-api": "https://github.com/kthjm/nicohoi-price-api#0",
    "orph": "^3.0.2",
    "react": "^16.2.0",
    "react-centpn": "^0.0.1",
    "react-dom": "^16.2.0",
    "react-parade": "^0.0.1",
    "react-player": "^1.1.1",
    "screenfull": "^3.3.2",
    "unist-util-find": "^1.0.1",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "chin": "^2.1.2",
    "chin-plugins-html-embed": "https://github.com/kthjm/chin-plugins-html-embed",
    "chin-plugins-imagemin": "https://github.com/kthjm/chin-plugins-imagemin",
    "chin-plugins-inkscape": "https://github.com/kthjm/chin-plugins-inkscape",
    "cross-env": "^5.1.3",
    "cross-var": "^1.1.0",
    "css-loader": "^0.28.9",
    "dotenv-webpack": "^1.5.4",
    "env-cmd": "^7.0.0",
    "execa": "^0.9.0",
    "flow-bin": "^0.63.1",
    "flow-typed": "^2.2.1",
    "http-server": "^0.11.1",
    "husky": "^0.14.3",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.10.2",
    "rehype": "^5.0.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.0"
  }
}
