  "repository": "niconico-hoik/web",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "nodule": "cross-env BABEL_ENV=cjs node -r @babel/register",
    "hook": "npm run nodule hook.js",
    "prestart": "cross-env HOOK_ENV=prestart npm run hook",
    "prebuild": "cross-env HOOK_ENV=prebuild npm run hook",
    "postbuild": "http-server _site -a localhost -p 7000 -o -c-1",
    "chin:pre": "cross-env BABEL_ENV=cjs chin -c -r @babel/register",
    "chin:dev": "cross-env BABEL_ENV=cjs chin watch -c -r @babel/register",
    "chin:mir": "cross-env BABEL_ENV=cjs chin -c -r @babel/register",
    "chin:pro": "cross-env BABEL_ENV=cjs chin -c -r @babel/register",
    "wpack:dll": "webpack",
    "wpack:dev": "cross-env BABEL_ENV=cjs webpack-dev-server",
    "wpack:pro": "cross-env BABEL_ENV=esm webpack",
    "wpack:non": "cross-env BABEL_ENV=esm webpack",
    "wpack:ana": "cross-env BABEL_ENV=esm webpack --display-used-exports",
    "wpack:spl": "cross-env BABEL_ENV=esm webpack",
    "start": "run-p chin:dev wpack:dev",
    "build": "run-s chin:mir wpack:pro",
    "build:mirror": "run-s chin:mir wpack:pro",
    "build:master": "run-s chin:pro wpack:pro"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "atra": "^1.2.3",
    "lonogara-sdk": "https://github.com/lonogara/sdk#0.0.8",
    "moment": "^2.22.2",
    "niconico-hoik-price": "https://github.com/niconico-hoik/price#0.0.4",
    "orph": "^3.0.7",
